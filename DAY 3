#include <stdio.h>

int main() {
    int n, k, comparisons = 0;
    
    // Input array size
    scanf("%d", &n);
    
    int arr[n];
    
    // Input array elements
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    
    // Input key to search
    scanf("%d", &k);
    
    // Linear search
    for (int i = 0; i < n; i++) {
        comparisons++; // increment comparisons
        if (arr[i] == k) {
            printf("Found at index %d\n", i);
            printf("Comparisons = %d\n", comparisons);
            return 0;
        }
    }
    
    // If key not found
    printf("Not Found\n");
    printf("Comparisons = %d\n", comparisons);
    
    return 0;
}
